install:
	install -d $(DESTDIR)/usr/share/qubes/repo-templates
	install -d $(DESTDIR)/etc/pki/rpm-gpg
	install -m 0644 qubes-templates.repo $(DESTDIR)/usr/share/qubes/repo-templates/qubes-templates.repo
	install -m 0644 RPM-GPG-KEY-qubes-4.1-primary $(DESTDIR)/etc/pki/rpm-gpg/RPM-GPG-KEY-qubes-4.1-primary
	install -m 0644 RPM-GPG-KEY-qubes-4.1-templates-community $(DESTDIR)/etc/pki/rpm-gpg/RPM-GPG-KEY-qubes-4.1-templates-community


